#, kde-format
msgid "specify the folder in the wallet <folder>"
msgstr "zehaztu zorroko <karpeta>"

#: main.cpp:48
#, kde-format
msgid "Folder"
msgstr "Karpeta"

#: main.cpp:51
#, kde-format
msgid "wallet"
msgstr "zorroa"

#: main.cpp:51
#, kde-format
msgid "The wallet to query"
msgstr "Itaundu beharreko zorroa"

#: main.cpp:61
#, kde-format
msgid "Missing argument"
msgstr "Argumentua falta da"

#: main.cpp:65
#, kde-format
msgid "Too many arguments given"
msgstr "Behar baino argumentu gehiago"

#: main.cpp:72
#, kde-format
msgid "Only one mode (list, read or write) can be set. Aborting"
msgstr ""
"Modu bakarra (zerrendatu, irakurri edo idatzi) ezarri daiteke. Galarazten"

#: main.cpp:78
#, kde-format
